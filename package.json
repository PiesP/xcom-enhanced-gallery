{
  "name": "xcom-enhanced-gallery",
  "version": "0.1.0",
  "description": "Enhanced media viewer and download functionality for X.com",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:watch": "vite build --mode development --watch",
    "build": "npm run build:clean && npm run typecheck && npm run lint && vite build --mode production",
    "build:dev": "npm run build:clean && vite build --mode development",
    "build:clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint ./src --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint ./src --fix",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "deps:check": "dependency-cruiser --validate .dependency-cruiser.cjs src",
    "deps:graph": "dependency-cruiser --output-type dot --validate .dependency-cruiser.cjs src | dot -T svg > docs/dependency-graph.svg",
    "deps:report": "dependency-cruiser --output-type html --validate .dependency-cruiser.cjs src > docs/dependency-report.html",
    "deps:json": "dependency-cruiser --output-type json --validate .dependency-cruiser.cjs src > docs/dependency-graph.json",
    "deps:all": "npm run deps:check && npm run deps:json && npm run deps:report && npm run deps:graph",
    "deps:validate": "npm run deps:check",
    "clean": "npm run build:clean",
    "validate": "npm run typecheck && npm run lint && npm run format:check && npm run test",
    "quality": "npm run validate && npm run deps:check",
    "package": "node scripts/package.js",
    "build:compile": "vite build --mode production"
  },
  "dependencies": {
    "@preact/signals": "^2.2.0",
    "fflate": "^0.8.2",
    "preact": "^10.26.8"
  },
  "devDependencies": {
    "@eslint/js": "9.28.0",
    "@preact/preset-vite": "^2.10.1",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/preact": "3.2.4",
    "@types/node": "^24.0.0",
    "@types/tampermonkey": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "cssnano": "7.0.7",
    "dependency-cruiser": "^16.10.2",
    "eslint": "9.28.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "graphviz": "^0.0.9",
    "jsdom": "26.1.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "terser": "5.42.0",
    "typescript": "5.8.3",
    "vite": "npm:rolldown-vite@latest",
    "vitest": "^3.2.3"
  }
}
