{
  "name": "xcom-enhanced-gallery",
  "version": "1.0.0",
  "description": "Media viewer and download functionality for X.com",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "pnpm quality && vite build",
    "build:dev": "pnpm quality && vite build --mode development",
    "build:fast": "vite build",
    "build:dev:fast": "vite build --mode development",
    "check": "tsc --noEmit --project tsconfig.build.json",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "fmt": "biome format --write ./src",
    "fmt:check": "biome format ./src",
    "biome:check": "biome check --write ./src",
    "quality": "pnpm check && biome check --write ./src"
  },
  "keywords": [
    "userscript",
    "x.com",
    "twitter",
    "gallery",
    "media"
  ],
  "author": "PiesP",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PiesP/xcom-enhanced-gallery"
  },
  "bugs": {
    "url": "https://github.com/PiesP/xcom-enhanced-gallery/issues"
  },
  "homepage": "https://github.com/PiesP/xcom-enhanced-gallery#readme",
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.12.1",
  "volta": {
    "node": "24.12.0",
    "pnpm": "10.12.1"
  },
  "dependencies": {
    "solid-js": "^1.9.10"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/node": "^22.19.2",
    "esbuild": "^0.25.12",
    "typescript": "^5.9.3",
    "vite": "8.0.0-beta.2",
    "vite-plugin-solid": "^2.11.10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite"
      ],
      "allowAny": [
        "vite"
      ]
    }
  }
}
