{
  "name": "xcom-enhanced-gallery",
  "version": "1.2.2",
  "description": "Media viewer and download functionality for X.com",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:fast": "vite build",
    "check": "tsgo --noEmit --project tsconfig.build.json || tsc --noEmit --project tsconfig.build.json",
    "lint": "biome lint ./src ./scripts vite.config.ts",
    "lint:fix": "biome lint --write ./src ./scripts",
    "fmt": "biome format --write ./src ./scripts",
    "fmt:check": "biome format ./src ./scripts",
    "prebuild": "pnpm quality",
    "preinstall": "npx only-allow pnpm",
    "quality": "pnpm check && biome check ./src ./scripts && biome lint vite.config.ts",
    "quality:fix": "pnpm check && biome check --write ./src ./scripts && biome lint vite.config.ts",
    "test:deps:check": "pnpm -C test run deps:check",
    "test:deps:sync": "pnpm -C test run deps:sync",
    "test:install": "pnpm -C test install",
    "test:unit": "pnpm -C test run test",
    "test:ct": "pnpm -C test run ct",
    "test:e2e": "pnpm -C test run e2e",
    "test:mut:priority": "pnpm -C test run mut:priority",
    "test:all": "pnpm -C test run test:all",
    "verify": "pnpm build && pnpm run test:all",
    "verify:fast": "pnpm build:fast && pnpm run test:all"
  },
  "keywords": [
    "userscript",
    "x.com",
    "twitter",
    "gallery",
    "media"
  ],
  "author": "PiesP",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PiesP/xcom-enhanced-gallery"
  },
  "bugs": {
    "url": "https://github.com/PiesP/xcom-enhanced-gallery/issues"
  },
  "homepage": "https://github.com/PiesP/xcom-enhanced-gallery#readme",
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.12.1",
  "volta": {
    "node": "24.12.0",
    "pnpm": "10.12.1"
  },
  "dependencies": {
    "solid-js": "^1.9.10"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@typescript/native-preview": "7.0.0-dev.20251214.1",
    "@types/node": "^25.0.2",
    "typescript": "^5.9.3",
    "vite": "8.0.0-beta.2",
    "vite-plugin-solid": "^2.11.10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite"
      ],
      "allowAny": [
        "vite"
      ]
    }
  }
}
